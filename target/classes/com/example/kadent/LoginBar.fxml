<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIcon?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="TOP_CENTER" prefHeight="20.0" prefWidth="95.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.login.LoginBarController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" />
   </padding>
   <children>
      <FontAwesomeIcon fx:id="collapseIcon" fill="#c4d71c" iconName="MINUS_SQUARE" onMouseClicked="#collapse" size="1.6em">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </FontAwesomeIcon>
      
            <FontAwesomeIcon fx:id="closeIcon" fill="#dd2727" iconName="TIMES" onMouseClicked="#close" size="1.6em">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </FontAwesomeIcon>
   </children>
</HBox>
